    <tu>
      <tuv lang="EN-US">
        <seg>A car that steers well on corners</seg>
      </tuv>
      <tuv lang="VI" changeid="hungv" changedate="20181008T182640Z" creationid="hungv" creationdate="20181008T182640Z">
